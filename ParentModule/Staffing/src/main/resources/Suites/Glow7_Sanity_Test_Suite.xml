<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Glow 7 Sanity Test Suite">

	<groups>
		<run>
			<include name="Sanity"></include>
		</run>
	</groups>
	<listeners>
		<listener
			class-name="listeners.DisableDataProviderIterationsForStaffing"></listener>
	</listeners>

	<test name="Gatekeepers Test Suite">
		<packages>
			<package
				name="tests.testcases.submodules.gatekeepers.features"></package>
		</packages>
	</test>
	<test name="Interview Test Suite">
		<packages>
			<package name="tests.testcases.submodules.interview.features"></package>
		</packages>
	</test>
</suite>
